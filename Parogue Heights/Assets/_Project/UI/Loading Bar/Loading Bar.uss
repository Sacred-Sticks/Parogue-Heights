VisualElement {
}

.loading_backdrop {
    flex: 1;
    background-color: rgb(25, 25, 25);
}

.unity-progress-bar__container {
    position: relative;
    top: 90px;
}

.unity-progress-bar__background {
    border-radius: 4px;
    border-color: rgb(50, 25, 25);
    background-color: rgb(100, 0, 0);
    min-height: 10px;
    max-height: 10px;
}

.unity-progress-bar__progress {
    border-radius: 2px;
    background-color: rgb(200, 0, 0);
}
