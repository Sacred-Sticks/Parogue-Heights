VisualElement {}

.root {
	flex: 1;
	display: flex;
	flex-direction: column-reverse;
}

.container {
	position: relative;
	bottom: 10px;
	height: 20px;
	flex-direction: row;
	justify-content: center;
}

.slot {
	background-color: rgba(50, 50, 50, 0.5);
	margin-left: 1px;
	margin-right: 1px;
	height: 15px;
	width: 15px;
	border-radius: 5px;
	border-width: 1px;
	border-color: rgba(0, 0, 0, 0.5);
}

.slot.active {
	background-color: rgba(25, 25, 100, 0.5) !important;
}